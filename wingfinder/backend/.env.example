# WingFinder API Environment Variables
# Copy this file to .env and fill in your actual API keys
# For Vercel deployment, add these as environment variables in your Vercel dashboard

# Flight search API keys
TEQUILA_API_KEY=your_tequila_api_key_here
TRAVELPAYOUTS_API_KEY=your_travelpayouts_api_key_here
TRAVELPAYOUTS_MARKER=your_travelpayouts_marker_here
DUFFEL_API_KEY=your_duffel_api_key_here
AMADEUS_API_KEY=your_amadeus_api_key_here
AMADEUS_API_SECRET=your_amadeus_api_secret_here

# Node environment
NODE_ENV=development